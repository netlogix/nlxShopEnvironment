<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service
                id="sd_shop_environment.dumper.shop_config_dumper"
                class="sdShopEnvironment\Dumper\ShopConfigDumper"
        >
            <argument type="service" id="models" />
        </service>
        <service
                id="sd_shop_environment.dumper.core_config_dumper"
                class="sdShopEnvironment\Dumper\CoreConfigDumper"
        >
            <argument type="service" id="models" />
            <argument type="service" id="config_writer" />
        </service>
        <service
                id="sd_shop_environment.dumper.theme_config_dumper"
                class="sdShopEnvironment\Dumper\ThemeConfigDumper"
        >
            <argument type="service" id="models" />
        </service>
        <service
                id="sd_shop_environment.dumper.productivity_mode_config_dumper"
                class="sdShopEnvironment\Dumper\ProductivityModeConfigDumper"
        >
            <argument type="service" id="models" />
        </service>
        <service
                id="sd.shop_environment.dumper.payment_methods_dumper"
                class="sdShopEnvironment\Dumper\PaymentMethodsDumper"
        >
            <argument id="models" type="service"/>
            <argument type="service" id="sd.shop_environment.serializer.serializer.payment_serializer"/>
        </service>
        <service
                id="sd.shop_environment.dumper.shipping_methods_dumper"
                class="sdShopEnvironment\Dumper\ShippingMethodsDumper"
        >
            <argument type="service" id="models"/>
            <argument type="service" id="sd.shop_environment.serializer.serializer.dispatch_serializer"/>
        </service>
        <service
                id="sd.shop_environment.dumper.documents_dumper"
                class="sdShopEnvironment\Dumper\DocumentsDumper"
        >
            <argument type="service" id="models"/>
        </service>
        <service
                id="sd.shop_environment.dumper.media_manager_dumper"
                class="sdShopEnvironment\Dumper\MediaManagerDumper"
        >
            <argument type="service" id="models"/>
        </service>
        <service
                id="sd.shop_environment.dumper.category_dumper"
                class="sdShopEnvironment\Dumper\CategoryDumper"
        >
            <argument type="service" id="models"/>
        </service>
    </services>
</container>
